{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate to help starting projects",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naabraz/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React",
    "JavaScript"
  ],
  "author": "Natalia Braz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naabraz/react-boilerplate/issues"
  },
  "homepage": "https://github.com/naabraz/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "react-test-renderer": "^16.6.3",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js"
  }
}
